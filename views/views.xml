<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="res_partner_contact_form" model="ir.ui.view">
      <field name="name">res.partner.inherited</field>
      <field name="model">res.partner</field>
      <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
          <field name="lang" position="after">  
            <field name="experience"/>
            <field name="origin_country"/>
            <field name="department"/>
          </field>
          <field name="name" position="after">  
            <field name="first_name" placeholder="e.g. Brandom"/>
            <field name="last_name" placeholder="e.g. Freeman" attrs="{'invisible':[('has_first_name', '=', 'False')], 'invisible':[('is_company', '=', 'False')]}" />
          </field>
        </field>
    </record>
  </data>
</odoo>



